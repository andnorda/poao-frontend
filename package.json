{
  "name": "poao-frontend",
  "version": "1.0.0",
  "description": "Express app som håndterer diverse funksjoner som er påkrevd av de fleste frontend applikasjoner",
  "main": "src/server.ts",
  "scripts": {
    "start": "dotenv -e env/basic.env ts-node src/server.ts",
    "start:auth": "dotenv -e env/auth.env ts-node src/server.ts",
    "start:proxy": "dotenv -e env/proxy.env ts-node src/server.ts",
    "start:gcs": "dotenv -e env/gcs.env ts-node src/server.ts",
    "start:redirect": "dotenv -e env/redirect.env ts-node src/server.ts",
    "test": "jest",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navikt/poao-frontend.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/navikt/poao-frontend/issues"
  },
  "homepage": "https://github.com/navikt/poao-frontend#readme",
  "dependencies": {
    "@google-cloud/storage": "^5.16.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "http-proxy-middleware": "^2.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.5",
    "mime-types": "^2.1.34",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.6",
    "url-join": "^4.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^16.11.11",
    "@types/node-fetch": "^2.5.12",
    "@types/url-join": "^4.0.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.1.1",
    "jest": "^27.4.3",
    "ts-jest": "^27.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
